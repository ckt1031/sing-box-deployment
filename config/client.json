{
    "log": {
        "level": "error"
    },
    "dns": {
      "servers": [
        {
          "tag": "dns_proxy",
          "type": "https",
          "server": "1.1.1.1",
          "detour": "Server"
        },
        {
          "tag": "dns_direct",
          "type": "tcp",
          "server": "223.5.5.5"
        }
      ],
      "rules": [
        {
          "rule_set": "geosite-geolocation-!cn",
          "server": "dns_proxy"
        },
        {
          "rule_set": "geosite-cn",
          "server": "dns_direct"
        }
      ],
      "final": "dns_proxy"
    },
    "inbounds": [
        {
            "type": "tun",
            "tag": "tun-in",
            "interface_name": "tun0",
            "address": "172.19.0.1/30",
            "auto_route": true,
            "strict_route": true,
            "stack": "mixed",
            "sniff": true
        }
    ],
    "outbounds": [
        {
            "type": "selector",
            "tag": "proxy",
            "outbounds": [
                "Server",
                "direct"
            ]
        },
        {
            "type": "vless",
            "tag": "Server",
            "server": "",
            "server_port": 443,
            "uuid": "",
            "flow": "xtls-rprx-vision",
            "tls": {
                "enabled": true,
                "server_name": "www.microsoft.com",
                "utls": {
                    "enabled": true,
                    "fingerprint": "chrome"
                },
                "reality": {
                    "enabled": true,
                    "public_key": "",
                    "short_id": ""
                }
            },
            "multiplex": {
                "protocol": "h2mux",
                "max_streams": 32,
                "padding": true,
                "brutal": {
                    "up_mbps": 1000,
                    "down_mbps": 100
                }
            },
            "packet_encoding": "xudp"
        },
        {
            "type": "direct",
            "tag": "direct"
        }
    ],
    "route": {
        "default_domain_resolver": {
          "server": "dns_proxy",
          "rewrite_ttl": 60,
          "client_subnet": "1.1.1.1"
        },
        "rules": [
            {
                "action": "sniff"
            },
            {
                "protocol": "dns",
                "action": "hijack-dns"
            },
            {
                "network": "tcp",
                "port": 853,
                "action": "reject"
            },
            {
                "network": "udp",
                "port": [
                    443,
                    853
                ],
                "action": "reject"
            },
            {
                "type": "logical",
                "mode": "and",
                "rules": [
                    {
                        "rule_set": "geoip-cn",
                        "invert": true
                    },
                    {
                        "rule_set": "geosite-geolocation-!cn"
                    }
                ],
                "outbound": "proxy"
            },
            {
                "type": "logical",
                "mode": "and",
                "rules": [
                    {
                        "rule_set": "geoip-cn"
                    },
                    {
                        "rule_set": "geosite-cn"
                    }
                ],
                "outbound": "direct"
            },
            {
                "rule_set": "geoip-cn",
                "outbound": "direct"
            },
            {
                "ip_is_private": true,
                "outbound": "direct"
            }
        ],
        "rule_set": [
            {
                "type": "remote",
                "tag": "geosite-geolocation-!cn",
                "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-geolocation-!cn.srs",
                "download_detour": "Server"
            },
            {
                "type": "remote",
                "tag": "geoip-cn",
                "url": "https://raw.githubusercontent.com/SagerNet/sing-geoip/rule-set/geoip-cn.srs",
                "download_detour": "Server"
            },
            {
                "type": "remote",
                "tag": "geosite-cn",
                "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-cn.srs",
                "download_detour": "proxy"
            }
        ],
        "final": "proxy",
        "auto_detect_interface": true
    },
    "experimental": {
        "cache_file": {
            "enabled": true,
            "path": "cache.db"
        }
    }
}